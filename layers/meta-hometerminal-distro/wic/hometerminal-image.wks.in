part /boot      --source bootimg-partition                                          --ondisk mmcblk0 --fstype=vfat --label boot     --active    --align 4096 --size 100
part /          --source rootfs                                                     --ondisk mmcblk0 --fstype=ext4 --label rootfs_A             --align 4096 --fixed-size 4G
part /          --source rootfs                                                     --ondisk mmcblk0 --fstype=ext4 --label rootfs_B             --align 4096 --fixed-size 4G
part mnt/data   --source rootfs             --rootfs-dir=${IMAGE_ROOTFS}/mnt/data   --ondisk mmcblk0 --fstype=ext4 --label data                 --align 1024 --size 500 --fsoptions "x-systemd.growfs"
